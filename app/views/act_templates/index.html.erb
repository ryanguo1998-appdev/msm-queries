<h1>List of all actors</h1>
<hr>
<table border=1>
  <tr>
      <th>ID</th>
      <th>Image</th>
      <th>Name</th>
      <th>Dob</th>
      <th></th>
    </tr>
<% @all_actors.each do |actor|%>
  <tr>
    <td><%= actor.id%></td>
    <td>
      <img src="http://robohash.org/<%=actor.image%>">
    </td>
    <td><%= actor.name%></td>
    <td><%= actor.dob%></td>
    <td><a href="/actors/<%=actor.id%>">Show details</a></td>
  </tr>
<% end %>
</table>